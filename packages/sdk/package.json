{
  "name": "@awcp/sdk",
  "version": "0.0.5",
  "description": "AWCP SDK - Delegator and Executor Daemon implementations",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./delegator": {
      "types": "./dist/delegator/index.d.ts",
      "import": "./dist/delegator/index.js"
    },
    "./delegator/client": {
      "types": "./dist/delegator/bin/client.d.ts",
      "import": "./dist/delegator/bin/client.js"
    },
    "./delegator/daemon": {
      "types": "./dist/delegator/bin/daemon.d.ts",
      "import": "./dist/delegator/bin/daemon.js"
    },
    "./executor": {
      "types": "./dist/executor/index.d.ts",
      "import": "./dist/executor/index.js"
    },
    "./server/express": {
      "types": "./dist/server/express/index.d.ts",
      "import": "./dist/server/express/index.js"
    }
  },
  "bin": {
    "delegator-daemon": "./dist/delegator/bin/daemon.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "clean": "rm -rf dist *.tsbuildinfo",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@awcp/core": "^0.0.5",
    "@a2a-js/sdk": "^0.3.5"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "keywords": [
    "awcp",
    "agent",
    "sdk",
    "delegator",
    "executor"
  ],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/anthropics/awcp.git",
    "directory": "packages/sdk"
  }
}
